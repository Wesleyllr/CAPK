{
  "expo": {
    "name": "Almeida",
    "slug": "pjFirebase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.lampier.pjfirebase",
      "supportsTablet": true
    },
    "android": {
      "package": "com.lampier.pjfirebase",
      "googleServicesFile": "./app/google-services.json",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logopapelariasemtexto.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logopapelariasemtexto.png"
    },
    "plugins": [
      "expo-image-picker",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logopapelaria.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b6ff1654-2f0d-471d-86a7-dbaaae99cb4f"
      }
    },
    "owner": "wesleyllr"
  }
}
